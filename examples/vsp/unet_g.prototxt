force_backward: true
name: "UNet"
layer {
  name: "data"
  type: "Data"
  top: "data"
  include {
    phase: TRAIN
  }
  data_param {
    source: "examples/vsp/layer2_2channel256-4590-float_train_lmdb"
    batch_size: 16
    backend: LMDB
  }
}
layer {
  name: "data_split"
  type: "Slice"
  bottom: "data"
  top: "data_A"
  top: "data_B"
  slice_param {
    slice_point: 1
    axis: 1
  }
}
layer {
  name: "layer4_downconv"
  type: "Convolution"
  bottom: "data_A"
  top: "layer4_downconv"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "layer4_downnorm"
  type: "BatchNorm"
  bottom: "layer4_downconv"
  top: "layer4_downnorm"
}
layer {
  name: "layer4_downact"
  type: "ReLU"
  bottom: "layer4_downnorm"
  top: "layer4_downact"
  relu_param {
    negative_slope: 0.20000000298023224
  }
}
layer {
  name: "layer3_downconv"
  type: "Convolution"
  bottom: "layer4_downact"
  top: "layer3_downconv"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "layer3_downnorm"
  type: "BatchNorm"
  bottom: "layer3_downconv"
  top: "layer3_downnorm"
}
layer {
  name: "layer3_downact"
  type: "ReLU"
  bottom: "layer3_downnorm"
  top: "layer3_downact"
  relu_param {
    negative_slope: 0.20000000298023224
  }
}
layer {
  name: "layer2_downconv"
  type: "Convolution"
  bottom: "layer3_downact"
  top: "layer2_downconv"
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "layer2_downnorm"
  type: "BatchNorm"
  bottom: "layer2_downconv"
  top: "layer2_downnorm"
}
layer {
  name: "layer2_downact"
  type: "ReLU"
  bottom: "layer2_downnorm"
  top: "layer2_downact"
  relu_param {
    negative_slope: 0.20000000298023224
  }
}
layer {
  name: "layer1_downconv"
  type: "Convolution"
  bottom: "layer2_downact"
  top: "layer1_downconv"
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "layer1_downnorm"
  type: "BatchNorm"
  bottom: "layer1_downconv"
  top: "layer1_downnorm"
}
layer {
  name: "layer1_downact"
  type: "ReLU"
  bottom: "layer1_downnorm"
  top: "layer1_downact"
  relu_param {
    negative_slope: 0.20000000298023224
  }
}
layer {
  name: "layer1_upconv"
  type: "Deconvolution"
  bottom: "layer1_downact"
  top: "layer1_upconv"
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "layer1_upnorm"
  type: "BatchNorm"
  bottom: "layer1_upconv"
  top: "layer1_upnorm"
}
layer {
  name: "layer1_upact"
  type: "ReLU"
  bottom: "layer1_upnorm"
  top: "layer1_upact"
}
layer {
  name: "layer2_concat"
  type: "Concat"
  bottom: "layer2_downact"
  bottom: "layer1_upact"
  top: "layer2_concat"
  concat_param {
    axis: 1
  }
}
layer {
  name: "layer2_upconv"
  type: "Deconvolution"
  bottom: "layer2_concat"
  top: "layer2_upconv"
  convolution_param {
    num_output: 256
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "layer2_upnorm"
  type: "BatchNorm"
  bottom: "layer2_upconv"
  top: "layer2_upnorm"
}
layer {
  name: "layer2_upact"
  type: "ReLU"
  bottom: "layer2_upnorm"
  top: "layer2_upact"
}
layer {
  name: "layer3_concat"
  type: "Concat"
  bottom: "layer3_downact"
  bottom: "layer2_upact"
  top: "layer3_concat"
  concat_param {
    axis: 1
  }
}
layer {
  name: "layer3_upconv"
  type: "Deconvolution"
  bottom: "layer3_concat"
  top: "layer3_upconv"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "layer3_upnorm"
  type: "BatchNorm"
  bottom: "layer3_upconv"
  top: "layer3_upnorm"
}
layer {
  name: "layer3_upact"
  type: "ReLU"
  bottom: "layer3_upnorm"
  top: "layer3_upact"
}
layer {
  name: "layer4_concat"
  type: "Concat"
  bottom: "layer4_downact"
  bottom: "layer3_upact"
  top: "layer4_concat"
  concat_param {
    axis: 1
  }
}
layer {
  name: "layer4_upconv"
  type: "Deconvolution"
  bottom: "layer4_concat"
  top: "layer4_upconv"
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "layer4_upnorm"
  type: "BatchNorm"
  bottom: "layer4_upconv"
  top: "layer4_upnorm"
}
layer {
  name: "layer4_upact"
  type: "ReLU"
  bottom: "layer4_upnorm"
  top: "layer4_upact"
}
layer {
  name: "conv_output"
  type: "Convolution"
  bottom: "layer4_upact"
  top: "conv_output"
  convolution_param {
    num_output: 1
    pad: 2
    kernel_size: 5
    stride: 1
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "output"
  type: "TanH"
  bottom: "conv_output"
  top: "output"
}
layer {
  name: "loss"
  type: "EuclideanLoss"
  bottom: "output"
  bottom: "data_B"
  top: "loss"
}
