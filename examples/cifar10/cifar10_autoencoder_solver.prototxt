net: "examples/cifar10/cifar10_autoencoder.prototxt"
test_interval: 500
base_lr: 0.001
lr_policy: "step"
gamma: 0.2
stepsize: 10000
display: 100
max_iter: 40000
weight_decay: 0.0005
snapshot: 10000
snapshot_prefix: "examples/cifar10/cifar10_autoencoder"
momentum: 0.9
solver_mode: GPU
solver_type: ADAM
