name: "DeconvolutionGenerator"
force_backward: true
layer {
  name: "noise"
  type: "RandVec"
  top: "noise"
  randvec_param {
    batch_size: 256
    dim: 128
    lower: -1.0
    upper: 1.0
  }
}
layer {
  name: "fc1"
  type: "InnerProduct"
  bottom: "noise"
  top: "fc1"
  inner_product_param {
    num_output: 8192
    weight_filler {
      type: "msra"
      
    }
  }
}
layer {
  name: "reshape"
  type: "Reshape"
  bottom: "fc1"
  top: "reshape_fc1"
  reshape_param {
    shape {
      dim: -1
      dim: 512
      dim: 4
      dim: 4
    }
  }
}
layer {
  name: "fc1_BN"
  type: "BatchNorm" include { phase: TRAIN}
  bottom: "reshape_fc1"
  top: "fc1_BN"
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }   
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }  
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }
  batch_norm_param {
    use_global_stats: false
    moving_average_fraction: 0.9
  }
}layer {
  name: "fc1_BN"
  type: "BatchNorm" include { phase: TEST}
  bottom: "reshape_fc1"
  top: "fc1_BN"
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }   
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }  
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }
  batch_norm_param {
    use_global_stats: true
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu_fc1"
  type: "ReLU"
  bottom: "fc1_BN"
  top: "relu_fc1"
}
layer {
  name: "deconv3"           
  type: "Deconvolution"
  bottom: "relu_fc1"
  top: "deconv3"
  convolution_param {
    num_output: 512 
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "msra"
      
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "deconv3_BN"
  type: "BatchNorm" include { phase: TRAIN}
  bottom: "deconv3"
  top: "deconv3_BN"
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }   
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }  
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }
  batch_norm_param {
    use_global_stats: false
    moving_average_fraction: 0.9
  }
}layer {
  name: "deconv3_BN"
  type: "BatchNorm" include { phase: TEST}
  bottom: "deconv3"
  top: "deconv3_BN"
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }   
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }  
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }
  batch_norm_param {
    use_global_stats: true
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu_deconv3"
  type: "ReLU"
  bottom: "deconv3_BN"
  top: "relu_deconv3"
}
layer {
  name: "deconv2"           
  type: "Deconvolution"
  bottom: "relu_deconv3"
  top: "deconv2"
  convolution_param {
    num_output: 256 
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "msra"
      
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "deconv2_BN"
  type: "BatchNorm" include { phase: TRAIN}
  bottom: "deconv2"
  top: "deconv2_BN"
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }   
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }  
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }
  batch_norm_param {
    use_global_stats: false
    moving_average_fraction: 0.9
  }
}
layer {
  name: "deconv2_BN"
  type: "BatchNorm" include { phase: TEST}
  bottom: "deconv2"
  top: "deconv2_BN"
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }   
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }  
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }
  batch_norm_param {
    use_global_stats: true
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu_deconv2"
  type: "ReLU"
  bottom: "deconv2_BN"
  top: "relu_deconv2"
}
layer {
  name: "deconv1"           
  type: "Deconvolution"
  bottom: "relu_deconv2"
  top: "deconv1"
  convolution_param {
    num_output: 128
    pad: 1
    kernel_size: 4
    stride: 2
    weight_filler {
      type: "msra"
      
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "deconv1_BN"
  type: "BatchNorm" include { phase: TRAIN}
  bottom: "deconv1"
  top: "deconv1_BN"
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }   
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }  
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }
  batch_norm_param {
    use_global_stats: false
    moving_average_fraction: 0.9
  }
}
layer {
  name: "deconv1_BN"
  type: "BatchNorm" include { phase: TEST}
  bottom: "deconv1"
  top: "deconv1_BN"
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }   
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }  
  param {  
    lr_mult: 0  
    decay_mult: 0  
  }
  batch_norm_param {
    use_global_stats: true
    moving_average_fraction: 0.9
  }
}
layer {
  name: "relu_deconv1"
  type: "ReLU"
  bottom: "deconv1_BN"
  top: "relu_deconv1"
}
layer {
  name: "conv1"           
  type: "Convolution"
  bottom: "relu_deconv1"
  top: "conv1"
  convolution_param {
    num_output: 3
    pad: 1
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "msra"
      
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "output"
  type: "TanH"
  bottom: "conv1"
  top: "output"
}